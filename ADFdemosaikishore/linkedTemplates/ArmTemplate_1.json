						}
					],
					"transformations": [
						{
							"name": "JoinEmpDataDepartmentData"
						}
					],
					"scriptLines": [
						"source(output(",
						"          EMPID as string,",
						"          EMPNAME as string,",
						"          EMPROLE as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false) ~> EmployeeData",
						"source(output(",
						"          DEPID as string,",
						"          DEPNAME as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
